//Check two given numbers and return true if one of the number is 80 or if their sum is 80

function checkSum80(x, y) 
{
  return ((x == 80 || y == 80) || (x + y == 80));
}
console.log(checkSum80(80, 80))
console.log(checkSum80(20, 80))
console.log(checkSum80(20, 20))
console.log(checkSum80(20, 30))

/* output
true
true
false
false */
